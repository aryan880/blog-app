<%- include("partials/header"); -%> 
<h1>Home</h1>
<p><%= homeStartingRoute%></p> 
<% list.forEach(function(element){ %>
 <% if(element.title.length>= 100) { %> 
    <h1><%= element.title.substring(0,100)+ "..." %> </h1>
    <p><%= element.body %> <a href="posts/<%= element.title %> "> Read More</a> </p>
 <% } else if(element.body.length>=100){ %>  
    <h1><%= element.title %></h1>
    <p><%= element.body.substring(0,100)+"..." %><a href="posts/<%= element.title %>"> Read More</a></p> 
 <% } else{ %> 
   <h1><%= element.title %> </h1>
   <p><%= element.body %><a href="posts/<%= element.title %>"> Read More</a></p>
 <% } %>
<% }); %>
<%- include("partials/footer"); -%> 